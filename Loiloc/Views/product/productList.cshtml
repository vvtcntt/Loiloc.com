
@{
     Layout = "~/Views/Shared/_LayoutDefault.cshtml";
}
@using PagedList.Mvc;
@model PagedList.IPagedList<Loiloc.Models.tblProduct>
@using Loiloc.Models
<div class="nVar_Product">
    <nav> @Html.Raw(ViewBag.nUrl)</nav>

</div>
<div id="descriptionCate">
    <h1>@ViewBag.h1 </h1>
    <div id="des">
        @Html.Raw(ViewBag.content)

    </div>
</div>

<div id="Product">
    <div id="Left_ListProduct">
        @{Html.RenderAction("productLeftPartial","product");}

    </div>
    <div id="Right_ListProduct">
       @Html.Raw(ViewBag.result)
        @Html.PagedListPager(Model, page => Url.Action("productList", new { page = page, sortOrder = ViewBag.CurrentSortOrder, sortName = ViewBag.CurrentSortName, sortLevel = ViewBag.CurrentSortLevel }), (PagedListRenderOptions)ViewBag.ship)


    </div>
 
    <div class="Clear"></div>
       
    <div class="Mxh" style="float: left; width: 100%; margin-top:5px">
        <div style="float:left" class="fb-like" data-send="true" data-layout="button_count" data-width="60"
             data-show-faces="true">
        </div>
        <div class="fb-send" style="float:left; margin:0px 5px" data-href="@Request.Url.ToString()"></div>
        <div id="g" style="float: left; margin: 0px 0px;width: auto">
            <g:plusone size="medium"></g:plusone>
        </div>
    </div>

    </div>

 